{
  "name": "alias-ui-framework",
  "files": [
    "dist"
  ],
  "main": "./dist/lib/alias-ui-framework.umd.js",
  "module": "./dist/lib/alias-ui-framework.es.js",
  "typings": "./dist/types/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/alias-ui-framework.es.js",
      "require": "./dist/alias-ui-framework.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "lint-staged": {
    "*.{ts,vue,js,tsx,jsx}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,vue,json}": "prettier --write"
  },
  "version": "0.0.14",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "rm -rf ./dist/* && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "compare-node-versions": "node scripts/nodeVersionComparator.mjs",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "typecheck": "vue-tsc --noEmit"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {}
  },
  "eslintIgnore": [
    "./src/vite-env.d.ts"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "dependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.11",
    "@storybook/addon-interactions": "^6.5.11",
    "@storybook/addon-links": "^6.5.11",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.11",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.1",
    "autoprefixer": "^10.4.9",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "8.7.1",
    "postcss": "^8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "standard": "^17.0.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.5.0",
    "vue-loader": "^16.8.3"
  },
  "engines": {
    "node": ">=16.17.0"
  },
  "volta": {
    "node": "16.17.0"
  }
}
