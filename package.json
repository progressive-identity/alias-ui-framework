{
  "name": "alias-ui-framework",
  "version": "0.4.0",
  "description": "Alias UI library",
  "files": [
    "dist"
  ],
  "main": "./dist/alias-ui-framework.umd.js",
  "module": "./dist/alias-ui-framework.es.js",
  "exports": {
    ".": {
      "import": "./dist/alias-ui-framework.es.js",
      "require": "./dist/alias-ui-framework.umd.js"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build": "vite build",
    "compare-node-versions": "node scripts/nodeVersionComparator.mjs",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{ts, js, vue}": "eslint --fix",
    "src/**/*.{js,jsx,ts,tsx, json}": "prettier --write",
    "test/**/*.{js,jsx,ts,tsx}": [
      "npm run test"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/progressive-identity/alias-ui-framework.git"
  },
  "keywords": [],
  "author": "Alias Tech Team",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/progressive-identity/alias-ui-framework/issues"
  },
  "homepage": "https://github.com/progressive-identity/alias-ui-framework#readme",
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack4": "^6.5.10",
    "@storybook/manager-webpack4": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "^6.5.10",
    "@vue/cli-service": "^5.0.8",
    "babel-loader": "^8.2.5",
    "husky": "^8.0.1",
    "tailwindcss": "^3.1.8",
    "vue-loader": "^16.8.3"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^3.1.0",
    "chalk": "^5.0.1",
    "postcss": "^8.4.16",
    "vite": "^3.1.0"
  },
  "engines": {
    "node": "16.17.0"
  },
  "volta": {
    "node": "16.17.0"
  }
}
